{
  "compilerOptions": {
    "target": "ES5",
    "module": "system",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "removeComments": false,
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "outFile": "gadget_board_frontend/bundle.js"

  },
  "exclude": [
    "node_modules"
  ],
  "files": [
    "gadget_board_frontend/**/*.ts",
    "typings/main.d.ts"
  ]
}