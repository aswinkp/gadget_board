{
  "compilerOptions": {
    "target": "ES5",
    "module": "system",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "removeComments": true,
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "outFile": "gadget_board_frontend/app.js"
  },
  "exclude": [
    "node_modules"
  ],
  "files": [
    "gadget_board_frontend/**/*.ts",
    "typings/main.d.ts"
  ]
}